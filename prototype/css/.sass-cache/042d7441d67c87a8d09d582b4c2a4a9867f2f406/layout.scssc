		background:url('../images/logo@2x.png') no-repeat;
		background-size:100% auto;
	}
	.slideout-menu li.dropdown > .drop-bttn {
		background-image:url('../images/hidestatus@2x.png');
		background-size:240px auto;
	}
	.main-view footer .row1 .pagewrap > .social-buttons > a {
		background:url('../images/socialsprites@2x.png');
		background-color:$dark-green;
		background-size:144px auto;
	}
}
/* Viewport Variances */
@media screen and (max-width:928px){
	section:not(.intro) .pagewrap {
		left:112px;
		right:112px;
		margin:0;
		width:auto;
	}
}
@media screen and (max-width:879px) {
	.services {
		@include columns(2);
		div {
			&:nth-of-type(2n){
				margin-bottom:39px;
			}
			&:nth-of-type(3n) {
				margin-bottom:0;
			}
		}
	}
}
@media screen and (max-width:862px) {
	section.help .link-array > .buttons > a {
		&.small, &.large {
			width:48%;
		}
		&:nth-of-type(2n) {
			margin-right:0;
		}
		&:nth-of-type(2n - 1){
			margin-left:0;
		}
		&:first-of-type, &:nth-of-type(2) {
			margin-bottom:16px;
		}
	}
}
@media screen and (max-width:768px) {
	.main-view .intro .pagewrap {
		width:auto;
		margin-left:0;
		left:112px;
		right:112px;
	}
}
@media screen and (max-width:652px) {
	.services {
		@include columns(1);
		div {
			&:nth-of-type(3n){
				margin-bottom:39px;
			}
		}
	}
	.main-view footer .row1 .pagewrap {
		> span {
			float:none;
			margin:0 auto;
			padding-top:0;
			width:100%;
			text-align:center;
		}
		> .social-buttons {
			display:block;
			float:none;
			text-align:center;
			margin:15px auto 0;
		}
	}
	.content.type1 {
		@include columns(1);
	}
}
@media screen and (max-width:619px) {
	.main-view .intro .nav-top .nav-right > a {
		display:none;
	}
}
@media screen and (max-width:611px) {
	section.help .link-array > .buttons > a {
		&.small, &.large {
			width:100%;
		}
		&:nth-of-type(2n), &:nth-of-type(2n - 1){
			margin:0 0 16px;
		}
		&:last-of-type {
			margin-bottom:0;
		}
	}
}
@media screen and (max-width:520px) {
	.main-view footer .row2 {
		.legal-links {
			display:none;
		}
		.copyright-notice {
			display:block;
			width:100%;
			text-align:center;
		}
	}
}
@media screen and (max-width:600px) {
	.sidenav {
		display:none;
	}
	.main-view .intro .pagewrap, section:not(.intro) .pagewrap {
		left:32px;
		right:32px;
	}
}
@media screen and (max-width:399px) {
	.nav-right span.hideable {
		display:none;
	}
	.main-view .intro {
		h1 {
			font-size:34px;
		}
		.nav-top .nav-right > button {
			width:40px;
			> span.symbol {
				margin-left:0;
			}
		}
	}
	.main-view footer .row1 .pagewrap > span > .review {
		display:block;
		width:87px;
		margin:10px auto;
	}
}
